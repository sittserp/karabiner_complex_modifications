{
  "title": "F5 Tenkey",
  "maintainers": [
    "IvanShamatov"
  ],
  "rules": [
    {
      "description": "Hold TAB to activate TenkeyMode",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "tab",
            "modifiers": {
              "optional": ["any"]
            }
          },
          "to_if_alone": [
            {
              "key_code": "tab"
            }
          ],
          "to_if_held_down": [
            {
              "set_variable": {
                "name": "tenkey_mode",
                "value": 1
              }
            }
          ],
          "parameters":{
            "basic.to_if_held_down_threshold_milliseconds": 0
          },
          "to_after_key_up": [
            {
              "set_variable": {
                "name": "tenkey_mode",
                "value": 0
              }
            }
          ]
        }
      ]
    },
    {
      "description": "F5 Tenkey: TenkeyMode on/off toggle",
      "manipulators": [
        {
          "from": {
            "key_code": "f5",
            "modifiers": {
              "mandatory": [

              ],
              "optional": [
                "any"
              ]
            }
          },
          "type": "basic",
          "to": {
            "set_variable": {
              "name": "tenkey_mode",
              "value": 1
            }
          },
          "conditions": [
            {
              "type": "variable_if",
              "name": "tenkey_mode",
              "value": 0
            }
          ]
        },
        {
          "from": {
            "key_code": "f5",
            "modifiers": {
              "mandatory": [

              ],
              "optional": [
                "any"
              ]
            }
          },
          "type": "basic",
          "to": {
